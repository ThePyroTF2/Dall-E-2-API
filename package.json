{
  "name": "dall-e_2_api",
  "version": "1.0.0",
  "description": "A node.js script that generates images using OpenAI's DALL-E 2 API",
  "main": "DALL-E.js",
  "homepage": "https://justDeeevin.github.io/DALL-E-2-API",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "package-windows": "npx pkg DALL-E-windows.js --targets latest-win-x64 -o 'C:\\Program Files\\DALL-E\\DALL-E.exe'",
    "package-mac": "npx pkg DALL-E-mac.js --targets latest-mac-x64 -o /usr/local/bin/DALL-E"
  },
  "author": "Devin",
  "license": "ISC",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "fs": "^0.0.1-security",
    "gh-pages": "^4.0.0",
    "open": "^8.4.0",
    "openai": "^3.1.0",
    "path": "^0.12.7",
    "pkg": "^5.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "simple-git": "^3.15.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "yargs": "^17.6.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
